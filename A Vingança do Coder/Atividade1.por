programa {
  funcao inicio() {
    
    inteiro numero1 , numero2
    escreva("insira um numero : ")
    leia(numero1)

    escreva("insira outro numero : ")
    leia(numero2)

    se (numero1 > numero2)
    escreva("O numero "+ numero1 +" é mairo")

    senao (numero2 < numero1)
    escreva("O numero "+ numero2 +" é maior")
  }
}